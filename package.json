{
  "name": "lor-tracker",
  "version": "1.5.0",
  "description": "A deck tracker for Legends of Runeterra",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder -p always"
  },
  "repository": "https://github.com/sethjwilliamson/LoRTracker",
  "keywords": [],
  "author": "lor-tracker.com",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.3.1",
    "electron-builder": "^22.7.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "darkmode-js": "^1.5.6",
    "electron-log": "^4.2.1",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "jquery": "^3.4.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "build": {
    "appId": "com.github.sethjwilliamson.LoRTracker",
    "win": {
      "publish": [
        {
          "provider": "github",
          "owner": "sethjwilliamson",
          "repo": "LoRTracker"
        }
      ]
    },
    "nsis": {
      "createStartMenuShortcut": true
    },
    "productName": "Legends of Runeterra Deck Tracker",
    "extraFiles": [
      {
        "from": "build",
        "to": "resources"
      }
    ]
  }
}
